{
  "manifest_version": 2,
  "name": "Firera Inspector",
  "version": "0.1",

  "description": "A tool for Firera discovery",
  "background": {
    "scripts": ["background.js"]
  },
  "devtools_page": "devtools.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ]
}
